{
  "name": "sara-query-assistant",
  "version": "1.0.0",
  "description": "SARA - Smart Analytics & Reporting Assistant for business data querying",
  "private": true,
  "keywords": [
    "analytics",
    "reporting",
    "sql",
    "ai",
    "dashboard"
  ],
  "author": "Amit Bhattacharya",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/vercel-build-setup.js && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "preview": "npm run build && npm run start",
    "clean": "rm -rf .next out dist",
    "pre-deploy": "node scripts/pre-deploy.js",
    "fix-prisma": "node scripts/fix-prisma-deployment.js",
    "setup-binaries": "node scripts/setup-prisma-binaries.js",
    "test-db": "node scripts/test-database-connection.js",
    "debug-prisma": "node scripts/debug-prisma-vercel.js",
    "vercel-setup": "node scripts/vercel-build-setup.js",
    "deploy": "npm run pre-deploy && npx vercel --prod",
    "db:generate": "prisma generate --schema=prisma/auth-schema.prisma && prisma generate --schema=prisma/business-schema.prisma",
    "db:push:auth": "prisma db push --schema=prisma/auth-schema.prisma",
    "db:push:business": "prisma db push --schema=prisma/business-schema.prisma",
    "db:studio:auth": "prisma studio --schema=prisma/auth-schema.prisma",
    "db:studio:business": "prisma studio --schema=prisma/business-schema.prisma",
    "postinstall": "prisma generate --schema=prisma/auth-schema.prisma && prisma generate --schema=prisma/business-schema.prisma"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.13",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.3.6",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@types/hoist-non-react-statics": "3.3.6",
    "ai": "^3.4.32",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.6.0",
    "embla-carousel-react": "8.5.1",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "mongodb": "^6.18.0",
    "mysql2": "^3.14.3",
    "next": "^15.2.4",
    "next-themes": "^0.4.4",
    "openai": "^4.0.0",
    "prisma": "^5.22.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.1.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.54.1",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.12.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.0",
    "vaul": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.2.4",
    "postcss": "^8.5.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "overrides": {
    "date-fns": "^3.6.0"
  }
}
